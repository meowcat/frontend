    "@testing-library/user-event": "~7.2.1",
    "@types/jest": "~24.9.1",
    "@types/node": "~12.12.39",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "apollo-boost": "~0.4.9",
    "dayjs": "~1.8.28",
    "formik": "~2.1.5",
    "graphql": "~15.0.0",
    "react": "~16.13.1",
